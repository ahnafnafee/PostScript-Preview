%!PS-Adobe-3.0
%%Pages: 3
%%Page: 1 1
%%BeginPageSetup
<< /PageSize [400 400] >> setpagedevice
%%EndPageSetup

% Page 1 - Red Circle
(Page 1: Drawing a red circle) ==

1 0 0 setrgbcolor
200 200 100 0 360 arc fill

0 setgray
/Helvetica findfont 24 scalefont setfont
100 50 moveto
(Page 1 - Red Circle) show

showpage

%%Page: 2 2
%%BeginPageSetup
<< /PageSize [400 400] >> setpagedevice
%%EndPageSetup

% Page 2 - Green Square
(Page 2: Drawing a green square) ==

0 1 0 setrgbcolor
100 100 200 200 rectfill

0 setgray
/Helvetica findfont 24 scalefont setfont
100 50 moveto
(Page 2 - Green Square) show

showpage

%%Page: 3 3
%%BeginPageSetup
<< /PageSize [400 400] >> setpagedevice
%%EndPageSetup

% Page 3 - Blue Triangle
(Page 3: Drawing a blue triangle) ==

0 0 1 setrgbcolor
newpath
200 300 moveto
100 100 lineto
300 100 lineto
closepath fill

0 setgray
/Helvetica findfont 24 scalefont setfont
90 50 moveto
(Page 3 - Blue Triangle) show

showpage
